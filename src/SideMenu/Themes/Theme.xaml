<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BgStarColour">#FFFFFF</Color>
    <Color x:Key="BgEndColour">#FFFFFF</Color>
    <Color x:Key="TitleColour">#000000</Color>
    <Color x:Key="DarkTitleColour">#4403739D</Color>
    <Color x:Key="TitleFaintColour">#8833A3CD</Color>
    <Color x:Key="Gray1Colour">#000000</Color>
    <Color x:Key="Blue1Colour">#000000</Color>
    <Color x:Key="BalckColour">#000000</Color>

    <SolidColorBrush x:Key="BgStartBrush"
                     Color="{StaticResource BgStarColour}" />
    <SolidColorBrush x:Key="BgEndBrush"
                     Color="{StaticResource BgEndColour}" />
    <SolidColorBrush x:Key="TitleBrush"
                     Color="{StaticResource TitleColour}" />
    <SolidColorBrush x:Key="DarkTitleBrush"
                     Color="{StaticResource DarkTitleColour}" />
    <SolidColorBrush x:Key="TitleFaintBrush"
                     Color="{StaticResource TitleFaintColour}" />
    <SolidColorBrush x:Key="Blue1Brush"
                     Color="{StaticResource Blue1Colour}" />
    <SolidColorBrush x:Key="Gray1Brush"
                     Color="{StaticResource Gray1Colour}" />
    <SolidColorBrush x:Key="White1Brush"
                     Color="{StaticResource BalckColour}" />

</ResourceDictionary>